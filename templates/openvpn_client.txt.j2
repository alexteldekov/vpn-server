client
dev tun
proto tcp
sndbuf 0
rcvbuf 0
remote {{ inventory_hostname }} 443
resolv-retry infinite
nobind
persist-key
persist-tun
remote-cert-tls server
#auth SHA512
#cipher AES-256-CBC
comp-lzo
setenv opt block-outside-dns
key-direction 1
verb 3
# Comment out not to Redirect Internet traffic
redirect-gateway def1 bypass-dhcp
